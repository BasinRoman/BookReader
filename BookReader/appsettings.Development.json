{
  
    "LiveReload": {
      "LiveReloadEnabled": true,
      "ClientFileExtensions": ".cshtml,.css,.js,.htm,.html,.ts,.razor,.custom",
      "ServerRefreshTimeout": 1000,
      "WebSocketUrl": "/__livereload",
      "LiveReloadScriptUrl": "/__livereloadscript",
      "WebSocketHost": null,
      "FolderToMonitor": "~/"
      // ... more options 
    },
 
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
